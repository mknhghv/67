-- åŸä½œè€…ï¼šå°è½©åˆ¶ä½œ | å®Œæ•´åŠŸèƒ½å…¨æ”¶éŒ„ + Velocity ç©©å®šå„ªåŒ–

local old
old = hookmetamethod(game:GetService("StarterGui"), "__namecall", newcclosure(function(self, ...)
    local method = getnamecallmethod()
    local args = {...}
    if method == "SetCore" and args[1] == "SendNotification" then
        local options = args[2]
        if type(options) == "table" and tostring(options.Title) == "é›¶è„šæœ¬" and tostring(options.Text) == "å°è½©åˆ¶ä½œ" then
            local modifiedOptions = table.clone(options)
            modifiedOptions.Text = "ç¾åŒ–ç‰ˆè¼‰å…¥ä¸­..."
            return old(self, modifiedOptions)
        end
    end
    return old(self, ...)
end))

