                BtnC.CornerRadius = UDim.new(0, 6)
                BtnC.Name = "BtnC"
                BtnC.Parent = Btn
                
                Btn.MouseButton1Click:Connect(function()
                    spawn(function()
                        Ripple(Btn)
                    end)
                    callback()
                end)
            end
            
            function section.Toggle(section, text, flag, default, callback)
                local callback = callback or function() end
                assert(text, "No text provided")
                assert(flag, "No flag provided")
                
                library.flags[flag] = default
                
                local ToggleModule = Instance.new("Frame")
                local ToggleBtn = Instance.new("TextButton")
                local ToggleBtnC = Instance.new("UICorner")
                local Tick = Instance.new("Frame")
                local TickC = Instance.new("UICorner")
                
                ToggleModule.Name = "ToggleModule"
                ToggleModule.Parent = Objs
                ToggleModule.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
                ToggleModule.BackgroundTransparency = 1.000
                ToggleModule.BorderSizePixel = 0
                ToggleModule.Position = UDim2.new(0, 0, 0, 0)
                ToggleModule.Size = UDim2.new(0, 428, 0, 38)
                
                ToggleBtn.Name = "ToggleBtn"
                ToggleBtn.Parent = ToggleModule
                ToggleBtn.BackgroundColor3 = zyColor
                ToggleBtn.BackgroundTransparency = zyColorTransparency  -- 半透明效果
                ToggleBtn.BorderSizePixel = 0
                ToggleBtn.Size = UDim2.new(0, 428, 0, 38)
                ToggleBtn.AutoButtonColor = false
                ToggleBtn.Font = Enum.Font.GothamBold  -- 更粗的字体
                ToggleBtn.Text = "   " .. text
                ToggleBtn.TextColor3 = TextColor  -- 亮白色
                ToggleBtn.TextSize = 16.000
                ToggleBtn.TextTransparency = 0
                ToggleBtn.TextXAlignment = Enum.TextXAlignment.Left
                
                ToggleBtnC.CornerRadius = UDim.new(0, 6)
                ToggleBtnC.Name = "ToggleBtnC"
                ToggleBtnC.Parent = ToggleBtn
                
                Tick.Name = "Tick"
                Tick.Parent = ToggleBtn
                Tick.AnchorPoint = Vector2.new(0, 0.5)
                Tick.BackgroundColor3 = beijingColor
                Tick.Size = UDim2.new(0, 0, 0, 0)
                Tick.Position = UDim2.new(0.925, 0, 0.5, 0)
                Tick.Visible = false
                
                TickC.CornerRadius = UDim.new(0, 4)
                TickC.Name = "TickC"
                TickC.Parent = Tick
                
                local function toggle()
                    library.flags[flag] = not library.flags[flag]
                    callback(library.flags[flag])
                    Tick.Visible = library.flags[flag]
                end
                
                if default then
                    toggle()
                end
                
                ToggleBtn.MouseButton1Click:Connect(function()
                    spawn(function()
                        Ripple(ToggleBtn)
                    end)
                    toggle()
                end)
            end
            
            function section.Keybind(section, text, flag, default, callback)
                local callback = callback or function() end
                local default = default or Enum.KeyCode.E
                assert(text, "No text provided")
                assert(flag, "No flag provided")
                
                library.flags[flag] = default
                
                local KeybindModule = Instance.new("Frame")
                local KeybindBack = Instance.new("TextButton")
                local KeybindBackC = Instance.new("UICorner")
                local KeybindValue = Instance.new("TextButton")
                local KeybindValueC = Instance.new("UICorner")
                local UIPadding = Instance.new("UIPadding")
                
                KeybindModule.Name = "KeybindModule"
                KeybindModule.Parent = Objs
                KeybindModule.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
                KeybindModule.BackgroundTransparency = 1.000
                KeybindModule.BorderSizePixel = 0
                KeybindModule.Position = UDim2.new(0, 0, 0, 0)
                KeybindModule.Size = UDim2.new(0, 428, 0, 38)
                
                KeybindBack.Name = "KeybindBack"
                KeybindBack.Parent = KeybindModule
                KeybindBack.BackgroundColor3 = zyColor
                KeybindBack.BackgroundTransparency = zyColorTransparency  -- 半透明效果
                KeybindBack.BorderSizePixel = 0
                KeybindBack.Size = UDim2.new(0, 428, 0, 38)
                KeybindBack.AutoButtonColor = false
                KeybindBack.Font = Enum.Font.GothamBold  -- 更粗的字体
                KeybindBack.Text = "   " .. text
                KeybindBack.TextColor3 = TextColor  -- 亮白色
                KeybindBack.TextSize = 16.000
                KeybindBack.TextTransparency = 0
                KeybindBack.TextXAlignment = Enum.TextXAlignment.Left
                
                KeybindBackC.CornerRadius = UDim.new(0, 6)
                KeybindBackC.Name = "KeybindBackC"
                KeybindBackC.Parent = KeybindBack
                
                KeybindValue.Name = "KeybindValue"
                KeybindValue.Parent = KeybindBack
                KeybindValue.BackgroundColor3 = Background
                KeybindValue.BackgroundTransparency = BackgroundTransparency  -- 半透明效果
                KeybindValue.BorderSizePixel = 0
                KeybindValue.Position = UDim2.new(0.763033211, 0, 0.131578952, 0)
                KeybindValue.Size = UDim2.new(0, 44, 0, 28)
                KeybindValue.AutoButtonColor = false
                KeybindValue.Font = Enum.Font.GothamBold  -- 更粗的字体
                KeybindValue.Text = default.Name
                KeybindValue.TextColor3 = TextColor  -- 亮白色
                KeybindValue.TextSize = 14.000
                KeybindValue.TextTransparency = 0
                
                KeybindValueC.CornerRadius = UDim.new(0, 6)
                KeybindValueC.Name = "KeybindValueC"
                KeybindValueC.Parent = KeybindValue
                
                UIPadding.Parent = KeybindBack
                UIPadding.PaddingRight = UDim.new(0, 6)
                
                local banned = { Enum.KeyCode.Unknown, Enum.KeyCode.Return }
                local shortNames = { 
                    LeftControl = "LCTRL",
                    RightControl = "RCTRL",
                    LeftShift = "LSHIFT",
                    RightShift = "RSHIFT",
                    MouseButton1 = "MB1",
                    MouseButton2 = "MB2"
                }
                
                local bindKey = default
                local oldKeyTxt = shortNames[bindKey.Name] or bindKey.Name
                
                services.UserInputService.InputBegan:Connect(function(inp, gpe)
                    if gpe then
                        return
                    end
                    if inp.UserInputType ~= Enum.UserInputType.Keyboard then
                        return
                    end
                    if inp.KeyCode ~= bindKey then
                        return
                    end
                    callback(bindKey.Name)
                end)
                
                KeybindValue.MouseButton1Click:Connect(function()
                    KeybindValue.Text = "..."
                    wait()
                    local key, uwu = services.UserInputService.InputEnded:Wait()
                    local keyName = tostring(key.KeyCode.Name)
                    if key.UserInputType ~= Enum.UserInputType.Keyboard then
                        KeybindValue.Text = oldKeyTxt
                        return
                    end
                    if table.find(banned, key.KeyCode) then
                        KeybindValue.Text = oldKeyTxt
                        return
                    end
                    wait()
                    bindKey = key.KeyCode
                    KeybindValue.Text = shortNames[keyName] or keyName
                    oldKeyTxt = KeybindValue.Text
                end)
                
                KeybindValue:GetPropertyChangedSignal("TextBounds"):Connect(function()
                    KeybindValue.Size = UDim2.new(0, KeybindValue.TextBounds.X + 30, 0, 28)
                end)
                
                KeybindValue.Size = UDim2.new(0, KeybindValue.TextBounds.X + 30, 0, 28)
            end
            
