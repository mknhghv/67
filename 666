    if gameProcessed then return end
    if input.KeyCode == Enum.KeyCode.Insert then
        if UIToggled then
            OrionLib:Destroy()
            UIToggled = false
            OrionLib:MakeNotification({Name = "UI å·²éš±è—", Content = "å†æŒ‰ Insert é¡¯ç¤º", Time = 3})
        else
            OrionLib:Init()
