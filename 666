Tab4:AddToggle({Name = "é€è¦–ç¸½é–‹é—œ", Default = false, Callback = function(v) ESPConfig.MainSwitch = v UpdateAllESP() end})
Tab4:AddToggle({Name = "é¡¯ç¤ºåå­— + è·é›¢", Default = false, Callback = function(v) ESPConfig.ShowNameDistance = v UpdateAllESP() end})
Tab4:AddToggle({Name = "é¡¯ç¤ºè¡€é‡", Default = false, Callback = function(v) ESPConfig.ShowHealth = v UpdateAllESP() end})
Tab4:AddToggle({Name = "å°„ç·šè¿½è¹¤", Default = false, Callback = function(v) ESPConfig.ShowTracer = v UpdateAllESP() end})

